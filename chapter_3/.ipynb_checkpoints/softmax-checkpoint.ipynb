   "source": [
    "import numpy as np\n",
    "def softmax(a):\n",
    "    c = np.max(a)\n",
    "    exp_a = np.exp(a-c) #prevent overflow\n",
    "    sum_exp = np.sum(exp_a)\n",
    "    \n",
    "    return exp_a / sum_exp"
   ]
  },
